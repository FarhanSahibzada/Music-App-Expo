{
  "expo": {
    "name": "Music-App",
    "slug": "Music-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconn.png",
    "userInterfaceStyle": "dark",
    "scheme": "Music-App",
    "splash": {
      "image": "./assets/splashhscreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.music.player",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splashhscreen.png",
        "backgroundColor": "#000000"
      },
      "package": "music.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router" 
    ],
    "experiments": {
      "typedRoutes": true,
      "tsConfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false  
      },
      "eas": {
        "projectId": "3a32a7ec-c659-44d3-b7a2-a5b47ddb56e5"
      }
    },
    "owner": "farhansahibzada"
  }
}
